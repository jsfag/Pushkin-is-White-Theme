{
  "type": "dark",
  "name": "Pushkin is Black",
  
  "tokenColors": [
    /* JS */
    {
      "name": "String",
      "scope": [
        "string.quoted",
        "string.template",
        "source.css entity.other.attribute-name", // CSS
      ],
      "settings": {
        "foreground": "#F92672" //"#FF628C" //"#fc3d7f" // "#fc3d7f" // "#c7254e" // "#E26A6A" // "#C0392B" // "#ff0068AA" // "#DCB777"
      }
    },
    {
      "name": "Support",
      "scope": [
        // "support.function",
        // "support.class"
      ],
      "settings": {
        "foreground": "#FFF900" // "#54d1ff"//"#cf6a4c"//"#ffe849"//"#d3423e"
      }
    },
    {
      "name": "Quotes",
      "scope": [
        "string.quoted punctuation.definition.string",
        // "string.template"
      ],
      "settings": {
        "foreground": "#EB939A" //"#E08283" // "#c7254e"//"#E08283" // "#E87E04" // "#E87E04"
      }
    },
    {
      "name": "String template",
      "scope": [
        "string.template punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#AE81FF" //"#9876AA" //"#8757ad"
      }
    },
    {
      "name": "Variable template",
      "scope": [
        "string.template variable",
        "string.template entity",
        "meta.object-literal.key variable.other.readwrite",
        "source.elixir.embedded",
        "keyword.operator.arithmetic"
      ],
      "settings": {
        "foreground": "#F8F8F8"
      }
    },
    {
      "name": "String template quotes",
      "scope": [
        "string.template punctuation.definition.string.template"
      ],
      "settings": {
        "foreground": "#ffe849"//"#c7254e"//"#fc3d7f"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant",
        "support.constant.property",
        "support.variable.object",
        "keyword.operator.ternary",
        "comment.wildcard"
      ],
      "settings": {
        "foreground": "#0088FF" //"#5cb4de" // "#5C97BF" //"#d35400"
      }
    },
    {
      "name": "Braces Array",
      "scope": [
        "meta.brace.square",
        "punctuation.definition.binding-pattern.array",
        "punctuation.section.array",
        "punctuation.definition.array", // JSON
      ],
      "settings": {
        "foreground": "#88FF00" // "#80FFBB" //"#A5C261" //"#FFB054"
      }
    },
    // {
    //   "name": "Array literal",
    //   "scope": [
    //     // "meta.array.literal"
    //   ],
    //   "settings": {
    //     "foreground": "#519F50"
    //   }
    // },
    {
      "name": "Array literal punctuation",
      "scope": [
        "meta.array.literal punctuation"
      ],
      "settings": {
        "foreground": "#F9F9F9"
      }
    },
    {
      "name": "Braces",
      "scope": [
        "meta.brace",
        "punctuation.definition",
        "punctuation.definition.parameters",
        "punctuation.terminator",
        "punctuation.accessor",
        "meta.objectliteral meta.block punctuation.definition.block",
        // "meta.block punctuation.definition.block",
        "punctuation.section.function"
      ],
      "settings": {
        "foreground": "#5F5A60" //"#5F5A60" //"#3B3B3B" //"#555555" "#F44336"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.section"
      ],
      "settings": {
        "foreground": "#F9F9F9"
      }
    },
    /* OBJECT */
    {
      "name": "Braces object",
      "scope": [
        "meta.var meta.objectliteral punctuation.definition",
        "meta.object-binding-pattern-variable punctuation.definition",
        "meta.objectliteral punctuation.definition",
        "meta.import punctuation.definition",
        "punctuation.definition.binding-pattern.object",
        "meta.block meta.objectliteral punctuation.definition.block",
        "keyword.operator.spread",
        "punctuation.section.scope",
        "punctuation.section.property-list.begin.bracket.curly", // CSS
        "punctuation.definition.dictionary", // JSON
      ],
      "settings": {
        "foreground": "#FFF900" //"#FFC600"
      }
    },
    {
      "name": "Object literal",
      "scope": [
        "meta.object-literal.key",
        "source.css support.type.property-name", // CSS
        "support.type.vendored", // CSS
        "string punctuation.support.type.property-name", // JSON
        "entity.other.attribute-name", // HTML
        "punctuation.definition.entity", // CSS
      ],
      "settings": {
        "foreground": "#FF9D00"
      }
    },
    {
      "name": "Object literal punctuation",
      "scope": [
        "meta.object-literal punctuation"
      ],
      "settings": {
        "foreground": "#F9F9F9"
      }
    },
    {
      "name": "Object literal as func. arg",
      "scope": [
        "meta.function variable"
      ],
      "settings": {
        "foreground": "#F9F9F9" //"#CC7833"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        // "fontStyle": "italic",
        "foreground": "#5F5A60" // "#FF0088" //"#664466"//"#555555" //"#746D73"
      }
    },
    {
      "name": "Keyword operator",
      "scope": [
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#5cb4de" //"#8757ad"//"#708394"
      }
    },
    {
      "name": "Keyword controls",
      "scope": [
        "keyword.control",
        "storage.control", // JS? Clojure!
        "storage.type",
        "keyword.operator.expression",
        "keyword.operator.new",
        "variable.language.this",
        "storage.modifier",
        "support.type.object", // Elixir
        "keyword.other.unit", // CSS
      ],
      "settings": {
        // "fontStyle": "bold",
        // "background": "#ffffff",
        "foreground": "#6bcbef"//"#4FC3F7"//"#5F5A60"//"#08fbcf"//"#9E2927" // "#BC3F3C" // //"#CF6A4C" // "#CC7832" //"#555555"//"#708394"
      }
    },

    /* JSDoc */
    {
      "name": "Storage",
      "scope": [
        "punctuation.definition.block.tag.jsdoc",
        "storage.type.class.jsdoc"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#708394"
      }
    },

 
    /* Rust */
    {
      "name": "String type",
      "scope": [
        "source.rust keyword.other",
        "source.rust keyword.operator.misc"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#555555"
      }
    },
    {
      "name": "Storage type",
      "scope": [
        "storage.type.core.rust"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#8757ad"
      }
    },
    {
      "name": "Storage modifier",
      "scope": [
        "storage.modifier.lifetime.rust"
      ],
      "settings": {
        "fontStyle": "normal",
        "foreground": "#8757ad"
      }
